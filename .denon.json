{
  "files": [
    "server.ts"
  ],
  "quiet": false,
  "debug": true,
  "fullscreen": true,
  "extensions": [
    ".js",
    ".ts",
    ".json"
  ],
  "interval": 500,
  "watch": [
  ],
  "deno_args": [
    "--allow-net",
    "--allow-read"
  ],
  "execute": {
    ".ts": [
      "deno",
      "run"
    ]
  },
  "fmt": false,
  "test": true
}
